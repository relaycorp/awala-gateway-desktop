{
  "name": "ui",
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "app/main.js",
  "scripts": {
    "postinstall": "run-s link-daemon",
    "link-daemon": "npm link --force ../daemon",
    "report-dependencies": "node report-dependencies.js",
    "build": "run-s --print-name report-dependencies tsc webpack",
    "webpack": "webpack --config ./webpack.config.js",
    "tsc": "tsc",
    "start": "electron ./app/main.js",
    "clean": "rimraf build test coverage dist app",
    "cov": "run-s build test:unit && opn coverage/lcov-report/index.html",
    "dist": "electron-builder --config electron-builder.yml --publish=never",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=warn' jest --coverage",
    "test:functional": "jest --config jest.config.functional.js --runInBand --detectOpenHandles"
  },
  "private": "true",
  "dependencies": {
    "abortable-iterator": "^3.0.0",
    "bufferutil": "^4.0.6",
    "daemon": "file:../daemon",
    "pino": "^8.14.2",
    "utf-8-validate": "^5.0.8",
    "verror": "^1.10.1",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.3",
    "@types/is-valid-domain": "0.0.1",
    "@types/jest": "^27.0.3",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/pino": "^7.0.5",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/verror": "^1.10.5",
    "@types/ws": "^8.2.2",
    "abort-controller": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^22.0.0",
    "electron-builder": "^24.0.0",
    "fetch-mock-jest": "^1.5.1",
    "html-webpack-plugin": "^5.5.0",
    "is-valid-domain": "0.1.6",
    "it-pipe": "^1.1.0",
    "it-ws": "^4.0.0",
    "jest": "^27.4.5",
    "jest-extended": "^2.0.0",
    "license-report": "^6.4.0",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.3.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0"
  }
}
