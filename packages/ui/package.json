{
  "name": "ui",
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "app/main.js",
  "scripts": {
    "postinstall": "run-s link-daemon",
    "link-daemon": "npm link --force ../daemon",
    "license-report": "node ./license-report.js",
    "build": "run-s license-report tsc webpack",
    "webpack": "webpack --config ./webpack.config.js",
    "tsc": "tsc -p tsconfig.json",
    "start": "npm run build && electron ./app/main.js",
    "clean": "rimraf build test coverage dist app",
    "cov": "run-s build test:unit && opn coverage/lcov-report/index.html",
    "dist": "electron-builder --config electron-builder.yml --publish=never",
    "test": "jest --coverage",
    "test:functional": "jest --config jest.config.functional.js --runInBand --detectOpenHandles",
    "test:ci:unit:jest": "jest --config jest.config.ci.js --coverage"
  },
  "private": "true",
  "dependencies": {
    "abortable-iterator": "^3.0.0",
    "bufferutil": "^4.0.3",
    "daemon": "file:../daemon",
    "pino": "^6.12.0",
    "utf-8-validate": "^5.0.5",
    "verror": "^1.10.0",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/is-valid-domain": "0.0.1",
    "@types/jest": "^26.0.24",
    "@types/mini-css-extract-plugin": "^2.0.1",
    "@types/pino": "^6.3.9",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/verror": "^1.10.5",
    "@types/ws": "^7.4.6",
    "abort-controller": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "fetch-mock-jest": "^1.5.1",
    "html-webpack-plugin": "^5.3.2",
    "is-valid-domain": "0.1.2",
    "it-pipe": "^1.1.0",
    "it-ws": "^4.0.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "license-report": "^4.5.0",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.7.2"
  }
}
